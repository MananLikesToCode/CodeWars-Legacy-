function XO(str) {
 str = str.toLowerCase();
if(str=="" ||ã€€str==null)return true;
var list = str.split(""); 
   var count = 0;
   var count2 = 0;
   for(var i = 0; i < list.length; i++){
       if(list[i] == "x"){
       count += 1; 
